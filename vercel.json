{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "https://sophius.officeofananyabirla.com/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }
  